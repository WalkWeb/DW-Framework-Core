{
  "name": "walkweb/dw-framework-core",
  "version": "1.0.1",
  "type": "library",
  "description": "Core Micro MVC Framework",
  "keywords": [],
  "authors": [
    {
      "name": "WalkWeb",
      "email": "av18775@list.ru"
    }
  ],
  "config": {
    "soft-packages": true
  },
  "require": {
    "php": ">=7.4.0",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-mysqli": "*",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "phpmailer/phpmailer": "^6.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "NW\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
