{
  "name": "walkweb/dw-framework-core",
  "version": "1.0.42",
  "license": "MIT",
  "minimum-stability": "stable",
  "type": "library",
  "description": "Core Micro MVC Framework",
  "keywords": [],
  "authors": [
    {
      "name": "WalkWeb",
      "email": "av18775@list.ru"
    }
  ],
  "config": {
    "soft-packages": true
  },
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-mysqli": "*",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "phpmailer/phpmailer": "^6.9",
    "ramsey/uuid": "^4.2",
    "gumlet/php-image-resize": "2.1.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "12.1.6",
    "friendsofphp/php-cs-fixer": "^3.75",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "WalkWeb\\NW\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
